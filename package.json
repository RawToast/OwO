{
  "name": "owo",
  "version": "1.4.1",
  "description": "OpenCode plugin with agents, background tasks for parallel execution, and smart orchestration",
  "keywords": [
    "agents",
    "ai",
    "background-tasks",
    "cli",
    "code-intelligence",
    "explorer",
    "keyword-triggers",
    "librarian",
    "llm",
    "lsp",
    "opencode",
    "oracle",
    "orchestration",
    "owo",
    "parallel",
    "plugin",
    "session-history",
    "thinking-modes",
    "toast-notifications",
    "todo-enforcer",
    "ui-planner",
    "ultrawork",
    "variants"
  ],
  "license": "MIT",
  "author": "rawtoast",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RawToast/owo.git"
  },
  "bin": {
    "owo": "./dist/cli/index.js"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun --format esm && bun build src/cli/index.ts --outdir dist/cli --target node --format esm && tsgo --emitDeclarationOnly",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build",
    "typecheck": "tsgo --noEmit",
    "lint": "oxlint",
    "format": "oxfmt",
    "dev": "bun run build && opencode"
  },
  "dependencies": {
    "@clack/prompts": "0.11.0",
    "@opencode-ai/plugin": "1.1.25",
    "@opencode-ai/sdk": "1.1.25",
    "@typescript/native-preview": "7.0.0-dev.20260120.1",
    "commander": "14.0.2",
    "picocolors": "1.1.1",
    "strip-json-comments": "5.0.3",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "bun-types": "1.3.6",
    "oxfmt": "0.24.0",
    "oxlint": "1.39.0"
  }
}
